{
  "components/common/header": "<header class=\"header\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\"></span>\r\n    </a>\r\n\r\n    <div class=\"header-logo--wrap\">\r\n        <div class=\"header-logo{{#if theme_settings.logo_size '===' 'original'}} logo-full-size-desktop{{/if}}{{#if theme_settings.logo_size_mobile '===' 'original'}} logo-full-size-mobile{{/if}}\">\r\n            \r\n            <div class=\"logo-and-toggle\">\r\n                <!--<div class=\"desktopMenu-toggle\">-->\r\n                <!--    <a href=\"javascript:void(0);\" class=\"mobileMenu-toggle\">-->\r\n                <!--        <span class=\"mobileMenu-toggleIcon\"></span>-->\r\n                <!--    </a>-->\r\n                <!--</div>-->\r\n                \r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"logo-wrap\">{{> components/common/store-logo}}</h1>\r\n                {{else}}\r\n                    <div class=\"logo-wrap\">{{> components/common/store-logo}}</div>\r\n                {{/if}}\r\n            </div>\r\n{{#if customer}}\r\n<div class=\"quickSearchWrap\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n    {{> components/common/quick-search}}\r\n</div>\r\n\r\n{{/if}}\r\n\r\n            <div class=\"navUser-wrap\">\r\n                {{> components/common/navigation}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n        <ul class=\"InfoPages {{#if customer}} hidethis {{/if}}\" id=\"myListPages\">\r\n        <li>\r\n            <a href=\"/\">Home</a>\r\n        </li>\r\n        <li>\r\n            <a href=\"/how-it-works\">How It Works</a>\r\n        </li>\r\n        <li>\r\n            <a href=\"/our-customers\">Our Customers</a>\r\n        </li>\r\n        <li>\r\n            <a href=\"/faq\">FAQ</a>\r\n        </li>\r\n        <li>\r\n            <a href=\"/team\">About Us</a>\r\n        </li>\r\n        <li class=\"mobileViewPage\">\r\n            <a href=\"/delivery-information/\">Delivery Information</a>\r\n        </li>\r\n        <li class=\"mobileViewPage\">\r\n            <a href=\"/news-updates/\">News & Updates</a>\r\n        </li>\r\n        </ul>\r\n        \r\n        {{> components/common/navigation-menu}}\r\n\r\n    </div>\r\n\r\n\r\n</header>\r\n{{#unless page_type '===' 'default'}}\r\n\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n\r\n{{/unless}}\r\n\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n",
  "components/common/quick-search": "<div>\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <button type=\"submit\" class=\"form-button\">\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-search\" /></svg>\n                    </i>\n                </button>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/navigation": "<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        <li class=\"navUser-item navUser-item--account\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-user\" /></svg>\n            </i>\n            {{#if customer}}\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> or\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            {{else}}\n\n\n\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span>{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            {{/if}}\n        </li>\n        {{> components/common/currency-selector}}\n        <li class=\"navUser-item navUser-item--mobile-search\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-search\" /></svg>\n            </i>\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\"\n                aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-cart\" /></svg>\n                </i>\n                (<span class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>)\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n</nav>\n",
  "components/common/navigation-menu": "<span class=\"mobile-closeNav\">\n    <i class=\"icon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-close-large\" /></svg>\n    </i>\n    close\n</span>\n<nav class=\"navPages {{#if customer}} loggedIn {{else}} notLoggedIn {{/if}}\" id=\"{{#if customer}}loggedIn{{else}}notLoggedIn{{/if}}\">\n    <ul class=\"navPages-list navPages-list--user 5765697a656e20596f756e67\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item navPages-item--currency\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item navPages-item--account\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-user\" /></svg>\n                    </i>\n                    &nbsp;\n                    {{lang 'common.account'}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.inbox}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                            >\n                                {{lang 'account.mobile_nav.messages'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.addresses}}\"\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\n                            >\n                                {{lang 'account.nav.addresses'}}\n                            </a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.payment_methods.all}}\"\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                                >\n                                    {{lang 'account.nav.payment_methods'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.wishlists.all}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                            >\n                                {{lang 'account.mobile_nav.wishlists'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.recent_items}}\"\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\n                            >\n                                {{lang 'account.nav.recently_viewed'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.details}}\"\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\n                            >\n                                {{lang 'account.nav.settings'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.auth.logout}}\"\n                               aria-label=\"{{lang 'common.logout'}}\"\n                            >\n                                {{lang 'common.logout'}}\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n        {{else}}\n            <li class=\"navPages-item\">\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg><use xlink:href=\"#icon-user\" /></svg>\n                </i>\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    {{lang 'common.or'}}\n                    <a class=\"navPages-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            </li>\n            <h3>{{lang 'footer.navigate'}}</h3>\n            <ul class=\"navPages-list navPages-list--user 5765697a656e20596f756e67\">\n                <li class=\"mobileViewPage\">\n                    <a href=\"/\">Home</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/how-it-works\">How It Works</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/our-customers\">Our Customers</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/faq\">FAQ</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/team\">About Us</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/delivery-information/\">Delivery Information</a>\n                </li>\n                <li class=\"mobileViewPage\">\n                    <a href=\"/news-updates/\">News & Updates</a>\n                </li>\n            </ul>\n\n        {{/if}}\n    </ul>\n    <ul class=\"navPages-list navPages-mega navPages-mainNav categoryListNav\">\n        {{#each categories}}\n        {{#if count '!=' 0}}\n            <li class=\"navPages-item\">\n                {{> components/common/navigation-list}}\n            </li>\n            {{/if}}\n        {{/each}}\n        {{#if theme_settings.show_content_navigation}}\n            {{#each pages}}\n                <li class=\"navPages-item{{#if @index '===' 0}} navPages-item--content-first{{/if}}\">\n                    {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                        {{> components/common/navigation-list-alternate}}\n                    {{/if}}\n                    {{#if ../theme_settings.navigation_design '===' 'mega'}}\n                        {{> components/common/navigation-list}}\n                    {{/if}}\n                </li>\n             {{/each}}\n        {{/if}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#and settings.phone_number theme_settings.header-phone}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navUser-action--phone\" href=\"tel:{{settings.phone_number}}\"><i class=\"fa fa-phone\" aria-hidden=\"true\"></i>&nbsp;{{settings.phone_number}}</a>\n            </li>\n        {{/and}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navPages-item\">\n                {{> components/common/social-links}}\n            </li>\n        {{/if}}\n    </ul>\n\n\n    \n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"column-menu navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        \n        {{#each children}}\n        {{#if count '!=' 0}}\n            <li  class=\"navPage-subMenu-item\">\n                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" id=\"cat{{id}}\" data-cateoryId=\"{{id}}\">{{name}}</a>\n                {{#if children}}\n                    <i class=\"icon navPages-action-moreIcon navPages-action-closed\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-plus\" /></svg></i>\n                    <i class=\"icon navPages-action-moreIcon navPages-action-open\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-minus\" /></svg></i>\n                {{/if}}\n                \n            </li>\n            {{/if}}\n            {{#if children}}\n                {{#each children}}\n                {{#if count '!=' 0}}\n                    <li class=\"navPage-childList-item\">\n                        <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" id=\"cat{{id}}\" data-cateoryId=\"{{id}}\">{{name}}</a>\n                    </li>\n                    {{/if}}\n                {{/each}}\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n    <li class=\"navUser-item navUser-item-currency\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-downselector-small\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        <li class=\"socialLinks-item\">\n            <a class=\"icon icon--Email\" href=\"mailto:hello@angrymonk.co.uk\" target=\"_blank\" rel=\"noopener\">\n               <img src=\"https://cdn11.bigcommerce.com/s-gy4celaekj/images/stencil/original/image-manager/envelope-regular-svg.png?t=1694090293&_gl=1*1v5ylwy*_ga*Mjk4MjM4NDEyLjE2ODg5NzM1ODU.*_ga_WS2VZYPC6G*MTY5NDA4OTI1MS44Ni4xLjE2OTQwODk2MjYuNDcuMC4w\" alt=\"\">\n            </a>\n        </li>\n    </ul>\n{{/if}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        <i class=\"icon navPages-secondLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i>\n                        {{name}}\n                        <i class=\"icon navPages-action-moreIcon navPages-action-more\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-plus\" /></svg></i>\n                        <i class=\"icon navPages-action-moreIcon navPages-action-less\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-minus\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\n                        <i class=\"icon navPages-secondLevel\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-arrow-right\" /></svg></i>\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}
